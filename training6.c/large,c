#include <stdio.h>

int main() {
    
    int numbers[] = {15, 42, 7, 89, 23, 67};
    int size = sizeof(numbers[0]);
    int max = numbers[0];
    for (int i = 1; i < size; i++) {
        if (numbers[i] > max) {
            max = numbers[i];
        }
    }
    printf("The largest element in the array is: %d\n", max);

    return 0;
}